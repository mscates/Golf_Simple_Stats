<% include ./partials/header %>

<div class="container w-50 mt-4">
  <h1>Sign Up</h1>
  <form action="/register" method="POST">
    <div class="form-group">
      <input
        class="form-control"
        type="text"
        name="username"
        placeholder="username"
      />
    </div>
    <div class="form-group">
      <input
        class="form-control"
        type="password"
        name="password"
        placeholder="password"
      />
    </div>

    <button>Sign Up</button>
  </form>
</div>

<% include ./partials/footer %>
