<% include ./partials/header %>

<div class="container w-100 mt-4">
  <h1>Login</h1>

  <form action="/login" method="POST">
    <div class="form-group">
      <input
        class="form-control"
        type="text"
        name="username"
        placeholder="username"
      />
    </div>
    <div class="form-group">
      <input
        class="form-control"
        type="password"
        name="password"
        placeholder="password"
      />
    </div>

    <button>Submit</button>
  </form>
</div>

<% include ./partials/footer %>
